{
  "name": "turbo-intro",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:docs": "turbo dev --filter=docs",
    "start:nairobi": "turbo dev --filter=nairobi",
    "start:kisumu": "turbo dev --filter=kisumu",
    "start:mombasa": "turbo dev --filter=mombasa",
    "start:electron": "turbo dev --filter=electron-app"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
